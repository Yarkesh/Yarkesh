{
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"no-unused-vars": "error",
		"prettier/prettier": "error"
	},
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	}
}
